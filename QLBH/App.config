<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Connection strings: replace SERVER_NAME, DATABASE_NAME and YOUR_SA_PASSWORD as needed -->
  <connectionStrings>
    <!-- Application default connection - Trusted/Windows Authentication -->
    <add name="QLBHConnection"
         connectionString="Data Source=SERVER_NAME;Initial Catalog=DATABASE_NAME;Integrated Security=True;MultipleActiveResultSets=True"
         providerName="System.Data.SqlClient" />

    <!-- SQL Server authentication (user = sa) -->
    <add name="SqlAuthConnection"
         connectionString="Data Source=SERVER_NAME;Initial Catalog=DATABASE_NAME;User ID=sa;Password=YOUR_SA_PASSWORD;MultipleActiveResultSets=True"
         providerName="System.Data.SqlClient" />
  </connectionStrings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>

  <appSettings>
    <!-- Application settings -->
    <add key="ApplicationName" value="QLBH" />
    <add key="ApplicationVersion" value="1.0" />
  </appSettings>
</configuration>
